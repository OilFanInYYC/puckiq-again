<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
    Player WoodMoney
  </h1>
</section>

<!-- Main content -->
<section class="content">
  <div class="box box-default">
    <div class="box-header with-border">
      <h3 class="box-title">Options</h3>
      <div class="box-tools pull-right">
        <button type="button" class="btn btn-box-tool" data-widget="collapse">
          <i class="fa fa-minus"></i>
        </button>
      </div>
    </div>
    <div class="box-body">
      <form>
        <div class="row">
          <div class="col-md-4 col-lg-3">
            <label>Player</label>
            <div class="form-group">
              <select name="q2playerid" id="pq-playername" class="form-control" style="width: 100%;">
              </select>
            </div>
          </div>
          <!-- /.col -->
          <div class="col-md-4 col-lg-3">
            <label>By Date Range</label>
            <div class="form-group">
              <div class="input-group">
                <div class="input-group-addon">
                  <i class="fa fa-calendar"></i>
                </div>
                <input name="q3daterange" id="pq-daterange" type="text" class="form-control pull-right" value="">
                <input name="q1datestart" type="hidden" value="" disabled="disabled">
                <input name="q1dateend" type="hidden" value="" disabled="disabled">
              </div>
            </div>
          </div>
          <!-- /.col -->
          <div class="col-md-4 col-lg-3">
            <label>By Season</label>
            <div class="form-group">
              <select name="q1season[]" id="pq-season" class="form-control" multiple="multiple" style="width: 100%;">
              </select>
            </div>
          </div>
          <!-- /.col -->
          <div class="col-md-4 col-lg-3">
            <label>Display Options</label>
            <div class="form-group">
              <select name="q3display[]" id="pq-display" class="form-control" multiple="multiple" style="width: 100%;">
              </select>
            </div>
          </div>
          <!-- /.col -->
          <div class="col-lg-offset-4 col-md-offset-2 col-lg-8 col-md-6">
            <label>&nbsp;</label>
            <div class="form-group">
              <button class="btn btn-default pull-right" type="submit">Go</button>
            </div>
          </div>
          <!-- /.col -->
        </div>
      </form>
    </div>
  </div>

  <div class="row">
    <div id="pq-wowydata" class="col-sm-12">
    </div>
  </div>
</section>
<!-- /.content -->